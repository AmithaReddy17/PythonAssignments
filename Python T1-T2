if counter == 6:
    print("Game over!")


# In[11]:


#11#

lucky_number = 9
counter = 1

while counter <= 5:
    guess = int(input("Type in the {} number: ".format(counter)))
    if guess == lucky_number:
        print("Good guess!")
        break
    else:
        print("Sorry but that was not very successful.")

